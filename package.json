{
    "name": "nest-sequelize-typescript",
    "version": "0.0.1",
    "description": "Nest + sequelize-typescript + JWT + Jest + Swagger",
    "keywords": [
        "nest",
        "nestjs",
        "nest.js",
        "sequelize",
        "sequelize-typescript",
        "orm",
        "nodejs",
        "node.js",
        "node",
        "typescript",
        "jwt",
        "jsonwebtoken",
        "passport",
        "swagger",
        "jest"
    ],
    "author": "Kent Loog",
    "license": "MIT",
    "scripts": {
        "build": "tsc -p tsconfig.build.json && npm run copyFile",
        "buildv2": "tsc -p tsconfig.build.json",
        "format": "prettier --write \"src/**/*.ts\"",
        "db:migrate": "ts-node node_modules/.bin/sequelize db:migrate",
        "db:drop": "ts-node node_modules/.bin/sequelize db:drop",
        "db:create": "ts-node node_modules/.bin/sequelize db:create",
        "db:seed-dev": "ts-node node_modules/.bin/sequelize db:seed:all",
        "db:seed-prod": "ts-node node_modules/.bin/sequelize db:seed:all --seeders-path db/seeders-prod",
        "db:reset": "npm run db:drop && npm run db:create && npm run db:migrate && npm run db:seed-dev && npm run db:seed-prod",
        "start": "ts-node -r tsconfig-paths/register src/main.ts",
        "start:dev": "nodemon",
        "tsc": "tsc",
        "start:dev2": "ts-node-dev --respawn --transpileOnly src/main.ts",
        "start:debug": "nodemon --config nodemon-debug.json",
        "prestart:prod": "rimraf dist && npm run build",
        "start:prod": "node dist/src/main.js",
        "lint": "tslint -p tsconfig.json -c tslint.json",
        "test": "jest --config ./test/jest-e2e.json",
        "copyFile": "cp package.json dist",
        "testw": "jest --watchAll"
    },
    "dependencies": {
        "@faker-js/faker": "^7.5.0",
        "@nestjs/axios": "^0.0.7",
        "@nestjs/common": "^8.4.4",
        "@nestjs/core": "^8.4.4",
        "@nestjs/passport": "^8.2.1",
        "@nestjs/platform-express": "^8.4.4",
        "@nestjs/schedule": "^1.1.0",
        "@nestjs/swagger": "~5.2.1",
        "@nestjs/throttler": "^2.0.1",
        "async": "^3.2.3",
        "cache-manager": "^3.6.1",
        "chalk": "4.1.2",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.13.2",
        "compression": "^1.7.4",
        "crypto-js": "^4.1.1",
        "exceljs": "^4.3.0",
        "file-saver": "^2.0.5",
        "fs-extra": "^10.1.0",
        "jsonwebtoken": "^8.5.1",
        "jszip": "^3.10.1",
        "jwt-decode": "^3.1.2",
        "moment": "^2.29.2",
        "moment-timezone": "^0.5.34",
        "mysql2": "^2.3.3",
        "npm": "^8.6.0",
        "pad": "^3.2.0",
        "passport": "^0.6.0",
        "passport-jwt": "^4.0.0",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^3.0.2",
        "rxjs": "^7.1.0",
        "sequelize": "^6.18.0",
        "sequelize-cli": "^6.4.1",
        "sequelize-typescript": "^2.1.3",
        "short-unique-id": "^4.4.4",
        "swagger-ui-express": "~4.3.0",
        "ts-node-dev": "^2.0.0",
        "uuid": "^8.3.2",
        "validator": "^13.7.0",
        "webp-converter": "^2.3.3",
        "winston": "^3.7.2"
    },
    "devDependencies": {
        "@nestjs/testing": "^8.4.4",
        "@swc/core": "^1.3.4",
        "@swc/helpers": "^0.4.11",
        "@types/async": "^3.2.12",
        "@types/base-64": "^1.0.0",
        "@types/cache-manager": "^3.4.3",
        "@types/compression": "^1.7.2",
        "@types/cron": "^1.7.3",
        "@types/crypto-js": "^4.1.1",
        "@types/express": "^4.17.13",
        "@types/file-saver": "^2.0.5",
        "@types/fs-extra": "^9.0.13",
        "@types/jest": "^27.4.1",
        "@types/jsonwebtoken": "^8.5.8",
        "@types/multer": "^1.4.7",
        "@types/node": "^17.0.23",
        "@types/passport": "^1.0.7",
        "@types/passport-jwt": "^3.0.6",
        "@types/sequelize": "^4.28.11",
        "@types/supertest": "^2.0.12",
        "@types/utf8": "^3.0.1",
        "@types/uuid": "^8.3.4",
        "jest": "^27.5.1",
        "nodemon": "^2.0.15",
        "prettier": "^2.6.2",
        "regenerator-runtime": "^0.13.9",
        "supertest": "^6.2.2",
        "ts-jest": "^27.1.4",
        "ts-node": "^10.7.0",
        "tsconfig-paths": "^3.14.1",
        "tslint": "^6.1.3",
        "typescript": "^4.6.3"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".spec.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}